    "dm": {
      "type": "list",
        "gold":{
          "desc": "Totals up currency rewards as gp and splits it among a party. First parameter must be a currency string with no spaces and the second is the number of players.",
          "usage": ["!gold <money string, no spaces> <party size>", "!gold 100pp20gp 2", "!gold 200gp20sp 4"]
        },
        "loot":{
          "desc": "Rolls for a random magic item of the specified rarity using the DMG and XGtE tables.",
          "usage": ["`!loot common` `!loot uncommon` `!loot rare` `!loot very rare` `!loot legendary`"],
          "footer": "Credit to @silverbass#2407"
        },
        "weather":{
          "desc": "Prints out a randomly generated weather status. Run !weather help for more details.",
          "usage": ["!weather [biome] [season]` `!weather [list/biomes]"],
          "footer": "Credit to @silverbass#2407 and @newtim#1227"
        }
    },
    "server": {
      "type": "list",
        "cwm": {
          "desc": "A search tool for displaying information on different facets of the server. You're probably using it right now.",
          "usage": ["!cwm [search terms]...", "!cwm alias server cwm", "!cwm meme doit"]
        },
        "sgold": {
          "desc": "This command is used for rolling your character's starting gold in FarWater. Starting gold depends on what level tier you are rolling your character at but for new players it is always level 3 (tier 1). If you don't supply a starting level then the command will assume you are rolling at level 3.\n\nThe command can also roll starting gold for if you choose to take no starting equipment. Just call the command while including your level 1 class to do this.",
          "usage": ["!sgold [level] [class]", "!sgold Wizard", "!sgold 16", "!sgold 4 Monk"]
        },
        "convert": {
          "desc": "A miscellaneous command to convert between mph and kph and Fahrenheit and Celsius.",
          "usage": ["!convert <number> <c|f|mph|kph>", "!convert 0 c", "!convert 12 mph", "!convert 70 f"]
        },
        "newchar": {
          "desc": "Rolls a new character just like the !randchar command. Prints out whether the stats rolled are valid under the current character creation rules.\n\nThe stat line is valid if you have at least 1 stat at 15 or higher and 1 other stat at 13 or higher. You can re-roll until you have 2 valid stat lines and may then choose a stat line from the prior rolls.",
          "usage": ["!newchar"]
        }
    },
    "silly": {
      "type": "list",
        "banhammer": {
          "desc": "Makes a truly destructive attack, may cause rocks to fall and you dieing.",
          "usage": ["!banhammer"]
        },
        "banhammer2": {
          "desc": "An even more destructive attack that could one shot a god, maybe.",
          "usage": ["!banhammer2"]
        },
        "boop": {
          "desc": "Boops the target.",
          "usage": ["!boop"]
        },
        "bap": {
          "desc": "Baps the target. Completely different from a boop.",
          "usage": ["!bap"]
        }
    }
  },
  "snippet": {
    "type": "list",
    "bless": {
      "desc": "Adds the Bless spell to your attacks rolls and saves.",
      "usage": ["!a Dagger bless", "!s wis bless"],
      "footer": "Credit to Toothless#7854"
    },
    "boom": {
      "desc": "Adds the effect of Booming Blade to an attack and displays the additional damage. Will scale with your level.",
      "usage": ["!a Whip boom"]
    },
    "d6": {
      "desc": "Rolls a separate d6 with the attack but doesn't add it to damage. For miscellaneous items.",
      "usage": ["!a Dagger d6"]
    },
    "d8": {
      "desc": "Rolls a separate d8 with the attack but doesn't add it to damage. For miscellaneous items.",
      "usage": ["!a Dagger d8"]
    },
    "d10": {
      "desc": "Rolls a separate d10 with the attack but doesn't add it to damage. For miscellaneous items.",
      "usage": ["!a Dagger d10"]
    },
    "gfb": {
      "desc": "Adds Green Flame Blade to an attack and displays the additional damage. Scales with your level and adds highest of int or chr for the spell casting mod.",
      "usage": ["!a Dagger gfb"],
      "footer": "Credit to Toothless#7854"
    },
    "guidance": {
      "desc": "Add Guidance to a skill check.",
      "usage": ["!c dexterity guidance"]
    },
    "gwm": {
      "desc": "Adds the Greater Weapon Master feat's effect to an attack.",
      "usage": ["!a Greatsword gwm"]
    },
    "hex": {
      "desc": "Adds the Hex effect to an attack.",
      "usage": ["!a Eldritch Blast hex"]
    },
    "holy": {
      "desc": "Adds the damage from the Holy Weapon spell to an attack.",
      "usage": ["!a Longsword holy"]
    },
    "mark": {
      "desc": "Adds the Hunter's Mark effect to an attack.",
      "usage": ["!a Longbow mark"]
    },
    "pwt": {
      "desc": "Adds the Pass Without Trace effect to a stealth check.",
      "usage": ["!c sneak pwt"]
    },
    "rage": {
      "desc": "Adds Rage damage to an attack. Uses the !level alias to work.",
      "usage": ["!a Greataxe rage"]
    },
    "resist": {
      "desc": "Adds the Resistance cantrip effect to a saving throw.",
      "usage": ["!s wis resist"]
    },
    "sharp": {
      "desc": "Add the Sharpshooter feat's effect to an attack.",
      "usage": ["!a Shortbow sharp"]
    },
    "sneak": {
      "desc": "Add sneak attack damage to an attack. Uses the !level alias to work.",
      "usage": ["!a Dagger sneak"]
    }
  },
