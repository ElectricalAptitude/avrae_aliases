      "type": "list",
      "counterspell":{
        "desc": "Performs a spell casting ability check to counter a spell of a certain level. If the spell slot used automatically cancels the other spell there is no need to run this alias. Does not subtract spell slots and only uses your spell mod for now (no adv or jack of all trades).",
        "aliasEx": ["!counterspell <level>", "!counterspell 9"]
      }
    },
