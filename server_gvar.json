  "game": {
    "type": "list",
    "heal": {
      "desc": "This command will heal your character by the amount provided. It is nearly the same as `!g hp +12` but will not allow overhealing. If no string is provided it will just heal 1 hp.",
      "aliasEx": ["!heal 12", "!heal 2d8+2", "!heal"]
    },
    "hitdice": {
      "desc": "This command will roll the specified hit die and heal you for that amount plus your CON modifier per die. \nThis command does require you to set up your level using `!level` beforehand. Custom counters will automatically be generated.",
      "aliasEx": ["!hitdice d6 1", "!hitdice 8 2", "!hitdice d12 4", "!hitdice"]
    },
    "hproll": {
      "desc": "Rolls for your hp using the class you supply. Won't roll anything if you do not give it a class. Give it a number at the end to roll multiple levels.",
      "aliasEx": ["!hproll wiz", "!hproll barb 2"]
    },
    "level": {
      "desc": "Sets character variables for use in the CWM aliases. Multiclass characters will need to call the command once for each class. Upon level up be sure to update your level using this command. DiceCloud users should not need to use this command.",
      "aliasEx": ["!level fighter 3", "!level Wizard 12"]
    },
    "rhitdie": {
      "desc": "This command restores your hit specified hit dice count by the amount given. Requires counters to be set up.",
      "aliasEx": ["!rhitdice d6 2", "!rhitdie 12 4"]
    },
    "money": {
      "desc": "Keeps track of currency. See `!money help` for more details. \nIf no coin is provided it will assume gp.",
      "aliasEx": ["!money [+/-] [amount] [coin]"],
      "footer": "Credit to @silverbass#2407"
    },
    "inv": {
      "desc": "Keeps track of inventory for a character. See `!inv help` for more details.",
      "aliasEx": ["!inv [+/-/?/help/delete] [item] [quantity]"],
      "footer": "Credit to @silverbass#2407"
    },
